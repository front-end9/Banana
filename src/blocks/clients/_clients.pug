mixin clients
    .clients
        .container
            h2.clients__title Clients 
            .row
                each val, index in ['', '', '', '']
                    .col-12.col-md-6.col-lg-3
                        .clients__img.d-flex.justify-content-center.align-items-ccenter
                            a(href="javascript:void(0)").clients__link
                                img(src=`./img/logo${index+1}.png`, alt='logo')

